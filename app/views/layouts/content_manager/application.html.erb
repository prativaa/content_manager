<!DOCTYPE html>
<html>
<head>
  <title>Content manager</title>
  <%= stylesheet_link_tag    "content_manager/application", media: "all" %>
  <%= javascript_include_tag "content_manager/application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="page-wrapper">
    <div id="page-header">
      <div id="header-logo">
        <a href="/admin">
        </a>
      </div>
    </div>
    <%= render 'partials/sidebar' %>
    <div id="page-content-wrapper">
      <div id="page-content">
        <%= yield %>
      </div>
    </div>

  </div>
</body>
</html>
